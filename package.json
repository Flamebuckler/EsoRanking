{
	"name": "eso-ranking",
	"version": "0.1.0",
	"description": "Static webapp and scraper for ESO leaderboards",
	"dependencies": {
		"express": "^4.18.2",
		"puppeteer": "^24.37.5",
		"styles": "github:flamebuckler/styles"
	},
	"scripts": {
		"scrape": "node scraper/index.js",
		"start": "node server.js",
		"dev": "node server.js",
